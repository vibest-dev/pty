{
  "name": "@vibest/pty-daemon",
  "version": "0.2.0",
  "description": "TypeScript SDK client for the vibest PTY daemon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibest/pty-daemon"
  },
  "keywords": [
    "pty",
    "terminal",
    "daemon",
    "sdk",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "test:sdk": "bun run build && vitest --run tests",
    "test": "bun run build && vitest --run",
    "publish:sdk:dry": "npm publish --dry-run --access public",
    "publish:sdk": "npm publish --access public",
    "prepublishOnly": "bun run test:sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0"
  }
}
